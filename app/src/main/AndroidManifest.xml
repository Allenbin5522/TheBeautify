<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dllo.thebeautiful">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ui.app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.SettingActivity"></activity>
        <activity android:name=".ui.activity.FeedbackActivity"></activity>
        <activity android:name=".ui.activity.CollectionActivity"></activity>
        <activity android:name=".ui.activity.FocusActivity"></activity>
        <activity android:name=".ui.activity.WishActivity"></activity>
    </application>

</manifest>